#Ejercicio41
ch = int(input("Ingrese el primer número = "))
ci = int(input("Ingrese el segundo número = "))
cj = int(input("Ingrese el tercer número = "))
if (ch == ci) or (ch == cj) or (ci == ch) or (ci == cj):
    print("Al menos 2 de los número ingresados son iguales.")
else:
    if (ch != ci) or (ch != cj) or (ci != ch) or (ci != cj):
        print("No hay al menos 2 número iguales.")

