#Ejercicio26
az=float(input("ingrese la primera nota "))
ba=float(input("ingrese la segunda nota "))
bb=float(input("ingrese la tercera nota "))
bc=float(input("ingrese la cuarta nota "))
bd=float(input("ingrese la quinta nota "))
az=az*0.15
ba=ba*0.2
bb=bb*0.15
bc=bc*0.3
bd=bd*0.2
if az+ba+bb+bc+bd<2:
    print("el estudiante no puede habilitar")
if az+ba+bb+bc+bd<3 and az+ba+bb+bc+bd>2:
    print("el estudiante reprobó")
if az+ba+bb+bc+bd>4.5:
    print("felicitaciones")
else:
    print("el estudiante aprobó")
    
