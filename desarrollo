#Ejercicio47
cp = int(input("Ingrese el primer número = "))
cq = int(input("Ingrese el segundo número = "))
if cq > cp:
    for cr in range(cp,cq):
        print("Los números naturales que hay entre los números ingresados son =" + " " +  str(cr))
else:
    print("El segundo número debe ser mayor que el primero porque es el límite.")
    
#Ejercicio48
cs = 0
ct = int(input("Ingrese el primer número = "))
cu = int(input("Ingrese el segundo número = "))
if cu > ct:
    for i in range(ct,cu):
        cs = cs + i
    print("La suma de los números naturales que hay entre los dos números ingresados es = ", cs)
else:
    print("El segundo número debe ser mayor que el primero porque es el límite.")
    
#Ejercicio49
cv = 1
cw = 0
while cv <= 10: #Ciclo
    cx = int(input("Ingrese la nota" + " " + str(cv) + " = "))
    if cv == 1:
        cw += cx
    if cv == 2:
        cw += cx
    if cv == 3:
        cw += cx
    if cv == 4:
        cw += cx
    if cv == 5:
        cw += cx
    if cv == 6:
        cw += cx
    if cv == 7:
        cw += cx
    if cv == 8:
        cw += cx
    if cv == 9:
        cw += cx
    if cv == 10:
        cw += cx
    cv += 1
print("El promedio de las notas es =" + " " +str(cw/10))

