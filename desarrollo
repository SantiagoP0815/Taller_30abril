#Ejercicio51
dd = 0
de = 0
df = 0
dg = 0
dh = 1
di = "si"
while di == "si":
    dj = int(input("Ingrese un número # " + str(dh) + " = "))
    di = str(input("¿Quieres ingresar más números? si o no = "))
    if di == "si":
        if dj % 2 == 0:
            de = de + dj
            dd += 1
        else:
            dg =  dg + dj
            df += 1
        dh += 1
dk = de / dd
dl = dg / df
print("El promedio de los números pares es = " + " " + str(dk) + " y " + "el promedio de los números impares es = " + " " + str(dl))

#Ejercicio52
