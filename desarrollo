#Ejercicio38
by = input( "Ingrese los tres número = ").split()
bz = int(by[0])
ca = int(by[1])
cb = int(by[2])
if (bz > ca) and (ca > cb):
    print("Está Disminuyendo.")
else:
    if (bz < ca) and (ca < cb):
        print("Está incrementando.")
    else:
        print("No se incrementa ni se disminuye.")
        
#Ejercicio39
cc = "true"
while cc == "true":
    cd = int(input("Ingrese el primer número = "))
    ce = int(input("Ingrese el segundo número = "))
    if (cd >= 0 and cd <= 5) and (ce >= 0 and ce <= 5):
        print("Los números" + " " + str(cd) + " y " + str(ce) + " " + "sí están en el rango.")
    else:
        print("Los números" + " " +  str(cd) + " y " + str(ce) + " = False")

