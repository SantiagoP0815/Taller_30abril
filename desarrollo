#Ejercicio29
bh=int(input("ingrese el primer número "))
bi=int(input("ingrese el segundo número "))
bj=int(input("ingrese el tercer número "))
if bh>bi:
    if bh>bj:
        print("el número mayor es",bh)
    if bh<bj:
        print("el número meyor es",bj)
if bi>bj:
    if bi>bh:
        print("el número mayor es",bi)
if bj>bh:
    if bj>bh:
        print("el número mayor es",bj)
if bh<bi:
    if bh<bj:
        print("el número menor es",bh)
    if bh<bj:
        print("el número menor es",bj)
if bi<bj:
    if bi<bh:
        print("el número menor es",bi)
if bj<bh:
    if bj<bh:
        print("el número menor es",bj)
        
#Ejercicio30
bk=int(input("ingrese el primer número "))
bl=int(input("ingrese el segundo número "))
bm=int(input("ingrese el tercer número "))
if bk+bl>bm:
    print("la suma del primer y segundo número es mayor que el tercero")
if bk+bl==bm:
    print("la suma del primer y segundo número es igual al tercero")
if bk+bl<bm:
    print("la suma del primer y segundo número es menor que el tercero")
    
